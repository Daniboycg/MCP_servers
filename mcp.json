{
    "mcpServers": {
      "n8n workflow creator": {
        "command": "node",
        "args": [
          "/Users/danielcarreon/Documents/AI/MCP_servers/n8n-workflow-builder-mcp-server/build/index.js"
        ]
      },
      "github": {
        "command": "npx",
        "args": [
